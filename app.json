{
  "name": "hf-ai-server",
  "description": "Deploy-ready HuggingFace AI proxy",
  "repository": "",
  "env": {
    "HF_TOKEN": {
      "description": "Your HuggingFace API token (hf_...)",
      "required": true
    },
    "HF_MODEL": {
      "description": "(optional) HuggingFace model name",
      "value": "microsoft/DialoGPT-medium"
    },
    "AI_TIMEOUT": {
      "description": "(optional) request timeout in ms",
      "value": "30000"
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  }
}
